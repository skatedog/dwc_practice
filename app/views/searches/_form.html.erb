<%= form_with url: searches_search_path, method: :get, local: true do |f| %>
  <%= f.text_field :keyword %>
  <%= f.select :model, [["Book","book"],["User","user"]] %>
  <%= f.select :parameter,[["完全一致","exact_match"],["前方一致","forward_match"],["後方一致","backward_match"],["部分一致","pattern_match"]] %>
  <%= f.submit "検索" %>
<% end %>